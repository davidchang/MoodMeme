extends layout

block content
  div.row-fluid
    .moodSet(ng-repeat='data in moodData')
      div(ng-repeat='mood in data')
        {{mood.type}} {{mood.value}}

block scripts
  script(src='//cdnjs.cloudflare.com/ajax/libs/angular.js/1.1.3/angular.min.js')
  script(src='/scripts/app.js')
  script(src='/scripts/view-my-mood-controller.js')
  script(src='/scripts/angular-local-storage.js')
  script(src='/scripts/services.js')
